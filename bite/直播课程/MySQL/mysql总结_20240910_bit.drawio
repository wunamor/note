    <mxGraphModel dx="863" dy="394" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="827" pageHeight="1169" math="0" shadow="0">
      <root>
        <mxCell id="0" />
        <mxCell id="1" parent="0" />
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-7" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;数据库&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 创建： create database 数据库名称;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 删除： drop database&amp;nbsp;&lt;span style=&quot;background-color: initial;&quot;&gt;数据库&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;名称;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 显示当前数据库数量： show databases;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;4. 使用： use&amp;nbsp;&lt;span style=&quot;background-color: initial;&quot;&gt;数据库&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;名称;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;5. 查看当前使用数据库：select database();&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="290" width="880" height="250" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-14" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;客户端&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 登入：mysql login -uroot -p&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 查看版本： select version();&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 登出： quit | exit&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;4. 查看警告: show warings;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="30" y="40" width="750" height="220" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-15" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;表&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 创建：create [if not exists] table 表名&lt;span style=&quot;background-color: initial;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;(字段)&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;2. 删除：drop [if exists] 表名;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;3. 查看表的结构：desc 表名;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;4. 查看该数据库中有哪些表：show tables;&lt;/span&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="570" width="750" height="220" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-16" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 35px;&quot;&gt;&lt;font style=&quot;font-size: 35px;&quot;&gt;CRUD&lt;/font&gt;&lt;/h1&gt;&lt;p style=&quot;font-size: 35px;&quot;&gt;&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="800" width="750" height="50" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-17" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 26px;&quot;&gt;I. 添加&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 单行添加： insert into 表名 values (值...);&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 多行添加： insert into 表名 values (值...)[,(值..)];&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 指点列名： insert into 表名(字段...) values(值...);&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="860" width="750" height="170" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-18" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;II. 查询&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 全列查询：select * from 表名;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 指定列查询： select 字段 from 表名;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 列为表达式查询：select 表达式 from 表名;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;4. 别名查询：select 字段/表达式 [as] 别名 from 表名; // &lt;b&gt;&lt;font color=&quot;#de0d0d&quot;&gt;别名有空格必须要有 &#39;&#39;&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;5. 去重查询：select distinct 字段 from 表名;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;6. 排序：select 字段 from 表名 order by 字段 asc | desc; // asc 升序 desc 降序&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;7. 条件查询：select * from table where 条件; // 从这里开始 表名变为 table&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;8. 区间查询：select * from table between 起始值 and 最终值;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;9. 模糊查询：select * from table like(&#39;%a_&#39;); // &#39;%&#39;可以有多个字符 &#39;_&#39;只能由一个字符&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;10. 分页查询： select * from table limit num;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;nbsp;&lt;span style=&quot;background-color: initial;&quot;&gt;select * from table limit num, len;&lt;/span&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;select * from table limit num offset len;&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="1040" width="750" height="520" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-20" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;更新&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;update table set 字段=&#39;&#39; where 条件 &lt;font color=&quot;#66ffb3&quot;&gt;order by asc|desc limit n;&lt;/font&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="30" y="1540" width="750" height="100" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-21" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;删除&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;delete from table where 条件&amp;nbsp;&lt;span style=&quot;color: rgb(102, 255, 179); background-color: initial;&quot;&gt;order by asc|desc limit n;&lt;/span&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;;&lt;/span&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="30" y="1650" width="750" height="110" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-23" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;数据库约束&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 非空约束：not null&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 唯一约束：unique&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 主键约束：primary key&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;4. 外键约束：foreign key (该表的字段)&amp;nbsp;&lt;span style=&quot;background-color: initial;&quot;&gt;references 主表(字段)&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;5. 默认约束：default&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="30" y="1790" width="750" height="250" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-24" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 34px;&quot;&gt;表的设计&lt;/h1&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="2060" width="750" height="60" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-25" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;I. 三大范式&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 第一范式：内部的元素不能再分&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 第二范式：&lt;font color=&quot;#34dd31&quot;&gt;消除了部分函数依赖&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 第三范式：&lt;font color=&quot;#36c459&quot;&gt;消除了传递依赖&lt;/font&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="2140" width="750" height="170" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-26" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;II. 关系模型&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 一对一：&lt;font color=&quot;#61da10&quot;&gt;根据具体业务，在相应的表上设置对应的引用&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 一对多：在多的那一张表上创建对应的字段&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 多对多：再创建一张表，包含其他两个表的主键&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="2310" width="750" height="170" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-28" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;聚合函数&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. count(*): 求出现个数&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. avg(字段): 求对应字段的平均值&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. sum(字段): 求对应字段的总和&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;span style=&quot;color: rgb(61, 194, 63); background-color: initial;&quot;&gt;4.&lt;/span&gt;&amp;nbsp;&lt;font color=&quot;#3dc23f&quot;&gt;min(字段): 求最小值&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;font color=&quot;#3dc23f&quot;&gt;5. max(字段): 求最大值&lt;/font&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="2490" width="750" height="240" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-29" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;分组查询 group by / having&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;select * from table group by 字段 having 条件&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;font color=&quot;#f0280a&quot;&gt;having 是分组后对虚拟的表进行筛选&lt;/font&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="2730" width="750" height="140" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-30" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 36px;&quot;&gt;联合查询&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="2880" width="750" height="60" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-31" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;I. 外连接&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. select * from table1, table2 where table1.id = table2.id;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. select * from table1 join table2 on table1.id = table2.id;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="2950" width="750" height="130" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-32" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;II. 内连接&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 左连接：select * from table1 left join table2 on table1.id = table2.id; // 保存左边表的全部值&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 右连接：select * from table1 right join table2 on table1.id = table2.id; // 保存右边表的全部值&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;情景：查询没有考试的学生...&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="3100" width="750" height="170" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-33" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;III. 子查询&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 查询单个：select * from table1 where id &lt;font color=&quot;#f05c26&quot;&gt;=&lt;/font&gt; (select id from table2 where ...);&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2.&amp;nbsp;&lt;span style=&quot;background-color: initial;&quot;&gt;查询多个：select * from table1 where id &lt;font color=&quot;#f50505&quot;&gt;in&lt;/font&gt; (select id from table2 where ...);&lt;/span&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="3287" width="750" height="143" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-34" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;&lt;span style=&quot;background-color: initial; font-size: 29px;&quot;&gt;IV. 自连接&lt;/span&gt;&lt;/h1&gt;&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;&lt;span style=&quot;font-size: 17px; font-weight: 400;&quot;&gt;select * from table t1, table t2 where ...;&lt;/span&gt;&lt;/h1&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="3440" width="750" height="110" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-35" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;&lt;span style=&quot;background-color: initial; font-size: 29px;&quot;&gt;&lt;font color=&quot;#3cf084&quot;&gt;V. 合并查询&lt;/font&gt;&lt;/span&gt;&lt;/h1&gt;&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;&lt;span style=&quot;font-size: 17px; font-weight: 400;&quot;&gt;1. 去重：select id, name from table1 union select id, name from table2;&lt;/span&gt;&lt;/h1&gt;&lt;div&gt;&lt;span style=&quot;font-size: 17px; font-weight: 400;&quot;&gt;2. 不去重：&lt;/span&gt;&lt;span style=&quot;background-color: initial; font-size: 17px;&quot;&gt;select id, name from table1 union &lt;font color=&quot;#f03f0a&quot;&gt;all&lt;/font&gt; select id, name from table2&lt;/span&gt;&lt;/div&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="39" y="3580" width="750" height="150" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-36" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;查询的执行顺序&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;FROM --&amp;gt; &lt;font color=&quot;#07f02e&quot;&gt;JOIN ON &lt;/font&gt;--&amp;gt; WHERE --&amp;gt; GROUP BY --&amp;gt; HAVING --&amp;gt; SELECT --&amp;gt; ORDER BY --&amp;gt; &lt;font color=&quot;#05f044&quot;&gt;LIMIT&lt;/font&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="40" y="3750" width="750" height="110" as="geometry" />
        </mxCell>
        <mxCell id="SYWQP4RQZJSJT3JnB4YY-37" value="&lt;h1 style=&quot;margin-top: 0px; font-size: 29px;&quot;&gt;&lt;font color=&quot;#f61313&quot;&gt;分析步骤（联合查询）&lt;/font&gt;&lt;/h1&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;1. 确认要查询哪些表（找到笛卡尔积）&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;2. 找到相对应的关系，过滤掉无效的数值&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;3. 用where等过滤出想要的结果&lt;/p&gt;&lt;p style=&quot;font-size: 17px;&quot;&gt;4. 对无关的字段进行舍弃&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="420" y="2800" width="764" height="197" as="geometry" />
        </mxCell>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
